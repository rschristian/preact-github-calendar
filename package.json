{
  "name": "preact-github-calendar",
  "version": "0.5.7",
  "source": "src/index.tsx",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/calendar.js",
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "license": "MIT",
  "description": "Preact Component that Displays a User's GitHub Contribution Calendar",
  "homepage": "https://rschristian.github.io/preact-github-calendar",
  "repository": "https://github.com/rschristian/preact-github-calendar",
  "keywords": [
    "preact",
    "github",
    "contribution",
    "calendar",
    "component"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "microbundle -f modern --no-sourcemap -o dist/calendar.js && mv dist/calendar.css dist/style.css",
    "build:docs": "yarn widget --no-watch && yarn twind --no-watch",
    "serve:dev": "sirv docs -p 3000 -D & yarn widget & yarn twind",
    "widget": "microbundle -f modern -i docs/dev/render.js --external none -o docs/calendar.js --no-sourcemap --watch",
    "twind": "twind-cli docs/dev/index.html -o docs/index.html -c docs/dev/twind.config.js",
    "format": "prettier --write --ignore-path .gitignore . '!docs/prism.js'"
  },
  "peerDependencies": {
    "preact": ">=10"
  },
  "dependencies": {
    "preact-hint": "^0.1.6"
  },
  "devDependencies": {
    "@rschristian/twind-cli": "^0.2.1",
    "chokidar-cli": "^3.0.0",
    "htm": "^3.1.0",
    "microbundle": "^0.13.3",
    "preact": "^10.5.14",
    "prettier": "^2.3.2",
    "prettier-config-rschristian": "^0.1.1",
    "sirv-cli": "^1.0.14",
    "typescript": "^4.3.5"
  },
  "prettier": "prettier-config-rschristian"
}
