{
  "name": "preact-github-calendar",
  "version": "0.5.9",
  "type": "module",
  "source": "src/index.tsx",
  "main": "dist/calendar.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/calendar.js",
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "license": "MIT",
  "description": "Preact Component that Displays a User's GitHub Contribution Calendar",
  "homepage": "https://rschristian.github.io/preact-github-calendar",
  "repository": "https://github.com/rschristian/preact-github-calendar",
  "keywords": [
    "preact",
    "github",
    "contribution",
    "calendar",
    "component"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "microbundle -f modern --no-sourcemap --tsconfig tsconfig.build.json && mv dist/calendar.css dist/style.css",
    "build:docs": "yarn build:widget --no-watch && mkdir -p build && wmr build --prerender && node scripts/post-build.js",
    "serve:dev": "wmr -p 3000",
    "serve:prod": "wmr serve -p 3000",
    "build:widget": "microbundle -f modern -i docs/utils/render.ts --external none -o docs --no-sourcemap --generateTypes=false --tsconfig tsconfig.build.json --watch",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "peerDependencies": {
    "preact": ">=10"
  },
  "dependencies": {
    "preact-hint": "^0.1.6"
  },
  "devDependencies": {
    "@twind/wmr": "^1.0.9",
    "dedent": "^0.7.0",
    "htm": "^3.1.0",
    "microbundle": "^0.14.2",
    "preact": "^10.7.0",
    "preact-feather": "^4.2.1",
    "preact-iso": "^2.3.0",
    "prettier": "^2.6.2",
    "prettier-config-rschristian": "^0.1.1",
    "prismjs": "^1.27.0",
    "twind": "^0.16.16",
    "typescript": "^4.6.3",
    "wmr": "^3.7.2"
  },
  "prettier": "prettier-config-rschristian"
}
