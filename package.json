{
  "name": "preact-github-calendar",
  "version": "0.5.0",
  "source": "src/Components/Calendar/index.tsx",
  "main": "dist/calendar.js",
  "module": "dist/calendar.module.js",
  "esmodule": "dist/calendar.modern.js",
  "umd:main": "dist/calendar.umd.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "description": "Preact Component that Displays a User's GitHub Contribution Calendar",
  "homepage": "https://rschristian.github.io/preact-github-calendar",
  "repository": "https://github.com/rschristian/preact-github-calendar",
  "keywords": [
    "preact",
    "github",
    "contribution",
    "calendar",
    "component"
  ],
  "sideEffects": false,
  "scripts": {
    "serve:dev": "preact watch -p 3000 --template src/template.html --refresh",
    "build": "microbundle --entry src/Components/Calendar/index.tsx --css-modules false --tsconfig tsconfig.build.json",
    "lint": "eslint '{src,test}/**/*.{js,ts,tsx}' --fix && prettier '{src,test}/**/*.css' './*.json' --write",
    "build:demo": "preact build --template src/template.html --no-sw",
    "serve:demo": "sirv build -p 3000 --single",
    "test": "jest --clearCache && jest --coverage"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "3.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "bulma": "0.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "gh-pages": "3.1.0",
    "jest": "^26.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-preset-preact": "^4.0.2",
    "microbundle": "^0.12.3",
    "postcss-nested": "4.2.3",
    "preact": "^10.4.7",
    "preact-cli": "^3.0.2",
    "preact-render-to-string": "^5.1.9",
    "prettier": "^2.0.5",
    "sirv-cli": "1.0.6",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "preact-hint": "^0.1.3"
  },
  "peerDependencies": {
    "preact": "*"
  }
}
