{
  "name": "preact-github-calendar",
  "version": "0.5.10",
  "type": "module",
  "source": "src/index.tsx",
  "main": "dist/calendar.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/calendar.js",
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "license": "MIT",
  "description": "Preact Component that Displays a User's GitHub Contribution Calendar",
  "homepage": "https://rschristian.github.io/preact-github-calendar",
  "repository": "https://github.com/rschristian/preact-github-calendar",
  "keywords": [
    "preact",
    "github",
    "contribution",
    "calendar",
    "component"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "microbundle -f modern --no-sourcemap --tsconfig tsconfig.build.json && mv dist/calendar.css dist/style.css",
    "build:dev": "microbundle -f modern --no-sourcemap --tsconfig tsconfig.build.json --no-compress --no-generateTypes -o docs",
    "build:docs": "astro build",
    "serve:dev": "astro dev",
    "serve:prod": "astro preview",
    "postinstall": "patch-package",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "peerDependencies": {
    "preact": ">=10"
  },
  "dependencies": {
    "preact-hint": "^0.2.0"
  },
  "devDependencies": {
    "@rschristian/astro-preact-twind": "^0.0.1",
    "@twind/preset-tailwind": "^1.0.0-next.37",
    "astro": "^1.0.0-beta.5",
    "dedent": "^0.7.0",
    "microbundle": "^0.14.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "preact": "^10.7.1",
    "preact-feather": "^4.2.1",
    "prettier": "^2.6.2",
    "prettier-config-rschristian": "^0.1.1",
    "prismjs": "^1.27.0",
    "twind": "^1.0.0-next.37",
    "typescript": "^4.6.3"
  },
  "prettier": "prettier-config-rschristian"
}
