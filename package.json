{
  "name": "preact-github-calendar",
  "version": "0.5.7",
  "source": "src/index.tsx",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/calendar.js",
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "license": "MIT",
  "description": "Preact Component that Displays a User's GitHub Contribution Calendar",
  "homepage": "https://rschristian.github.io/preact-github-calendar",
  "repository": "https://github.com/rschristian/preact-github-calendar",
  "keywords": [
    "preact",
    "github",
    "contribution",
    "calendar",
    "component"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "microbundle -f modern --no-sourcemap",
    "postbuild": "mv dist/preact-github-calendar.js dist/calendar.js && mv dist/preact-github-calendar.css dist/style.css",
    "serve:dev": "sirv docs -p 3000 -D & yarn dev:lib & yarn dev:twind",
    "dev:lib": "yarn build -o docs/dev --watch & chokidar --initial 'docs/dev/preact-github-calendar.js' -c 'node docs/dev/post-build.js'",
    "dev:twind": "chokidar --initial 'docs/dev/{index.html,twind-processor.js}' -c 'node docs/dev/twind-processor.js'",
    "format": "prettier '{src,docs}/**/*.{html,css,js}' '*.json' --write"
  },
  "peerDependencies": {
    "preact": ">=10"
  },
  "dependencies": {
    "preact-hint": "^0.1.6"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "html-minifier-terser": "^5.1.1",
    "microbundle": "^0.13.3",
    "preact": "^10.5.14",
    "prettier": "^2.3.2",
    "prettier-config-rschristian": "^0.1.1",
    "sirv-cli": "^1.0.12",
    "twind": "^0.16.16",
    "typescript": "^4.3.5"
  },
  "prettier": "prettier-config-rschristian"
}
